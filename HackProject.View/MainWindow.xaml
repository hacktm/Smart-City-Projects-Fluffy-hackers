<Window x:Class="HackProject.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="1024" Width="700">
    <StackPanel Margin="0,0,-8,-20">
        <Button Name="StartButton" Height="23" Content="Start" Click="StartRead"/>
        <Button Name="StopButton" Height="23" Content="Stop" Click="StopRead"></Button>
        <Button Name="StartTestButton" Height="23" Content="Start Test" Click="StartTestRead"/>
        <Button Name="StopTestButton" Height="23" Content="Stop Test" Click="StopTestRead"></Button>
        <ListBox Name="ListOfPositions"></ListBox>
    </StackPanel>
</Window>
